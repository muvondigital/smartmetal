# Test Environment Configuration
# Copy this file to .env.test and fill in with your test database credentials

# Test Database URL (use a separate test database!)
DATABASE_URL=postgresql://username:password@host:5432/pricer_test

# Azure OpenAI Configuration (optional for most tests)
AZURE_OPENAI_ENDPOINT=your_test_endpoint
AZURE_OPENAI_API_KEY=your_test_key
AZURE_OPENAI_DEPLOYMENT_NAME=your_test_deployment

# Azure Document Intelligence (optional for most tests)
AZURE_DOCUMENT_INTELLIGENCE_ENDPOINT=your_test_endpoint
AZURE_DOCUMENT_INTELLIGENCE_KEY=your_test_key

# Server Configuration
PORT=4001
NODE_ENV=test
