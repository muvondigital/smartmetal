{
  "name": "smartmetal-cpq-backend",
  "version": "0.1.0",
  "description": "SmartMetal CPQ Backend API - A product from Muvon Digital, an innovation arm of Muvon Energy",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node src/index.js",
    "check:env": "node -e \"require('./src/config/env')\"",
    "migrate": "node src/db/runAllMigrations.js",
    "migrate:v2": "node scripts/runMigrationV2.js",
    "seed:dev": "npm run seed:tenants-users && npm run seed:metasteel-demo",
    "seed:tenants-users": "node scripts/seedTenantsAndUsers.js",
    "seed:admin": "node scripts/seedAdminUser.js",
    "seed:metasteel-demo": "node scripts/seedMetaSteelDemoData.js",
    "seed:nsc": "node scripts/seedNscStandardCatalog.js",
    "seed:pipes": "node scripts/runPipeCatalogSetup.js",
    "seed:flanges": "node scripts/runFlangeCatalogSetup.js",
    "seed:catalogs": "node scripts/runCatalogSetup.js",
    "reset:metasteel": "node scripts/resetAndSeedMetaSteelDemo.js",
    "backup": "node scripts/automated-backup.js",
    "backup:metasteel": "node scripts/backupMetaSteelData.js",
    "db:backup": "pg_dump --format=custom --no-owner --clean --file=backup.dump $DATABASE_URL",
    "db:restore": "pg_restore --clean --no-owner --dbname=$DATABASE_URL backup.dump",
    "health:check": "node scripts/verifyMetaSteelDemoHealth.js",
    "verify:all": "npm run test:smoke && npm run health:check",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:smoke": "jest --testPathPattern=smoke --coverage=false --verbose",
    "test:critical": "jest --testPathPattern='(smoke|integration)' --coverage=false",
    "test:assistant": "jest --runInBand assistant",
    "export:training": "node scripts/exportTrainingData.js",
    "export:data-integrity": "node scripts/exportDataIntegrityGaps.js",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.{js,json}\"",
    "__deprecated__": "The scripts below are deprecated. Use canonical scripts above.",
    "test-connection": "node test-supabase-connection.js",
    "db:backup:auto": "node scripts/automated-backup.js",
    "seed:pipes:csv": "node ../scripts/db/seed_pipes_csv.js",
    "seed:fittings": "node ../scripts/db/seed_fittings.js",
    "seed:flanges:old": "node ../scripts/db/seed_flanges.js",
    "seed:fasteners": "node ../scripts/db/seed_fasteners.js",
    "seed:mto:wphpdn": "node scripts/seed_mto_wphpdn_pages26_32.js",
    "seed:nsc:standard": "node scripts/seedNscStandardCatalog.js",
    "clean:nsc:materials": "node scripts/cleanNscMaterials.js",
    "seed:kgsb-gratings": "node ../scripts/db/seed_kgsb_gratings.js",
    "seed:tariff-keywords": "node scripts/seedTariffKeywords.js",
    "seed:regulatory": "node src/db/seeds/regulatory/regulatorySeeder.js",
    "generate:hs-seed": "node scripts/tools/generateHsCodesSeed.js",
    "seed:hs": "node scripts/db/seed_hs_codes.js",
    "seed:countryProfiles": "node src/db/seeds/seedCountryProfiles.js",
    "verify:metasteel": "node scripts/verifyMetaSteelData.js",
    "verify:metasteel:health": "node scripts/verifyMetaSteelDemoHealth.js",
    "nuke:nsc-rfqs": "cross-env NODE_ENV=development node scripts/nukeNscRfqData.js",
    "seed:nsc-rfqs": "cross-env NODE_ENV=development node scripts/seedNscRealisticRfqs.js",
    "verify:tenant-data": "node scripts/printRfqSummaryByTenant.js",
    "migrate:sku": "node src/db/migrations/002_add_sku_columns.js",
    "migrate:pipes-constraint": "node src/db/migrations/004_add_pipes_unique_constraint.js",
    "backfill:sku": "node -r ts-node/register ../scripts/maintenance/backfill_sku.js",
    "dry-run:sku": "node -r ts-node/register ../scripts/maintenance/dry_run_sku.js",
    "analyze:sku": "ts-node ../scripts/maintenance/analyze_sku_issues.ts",
    "reset:rfq-history": "node ../scripts/db/resetTenantRfqHistory.js",
    "test:migration": "node ../scripts/db/testMigration.js",
    "test:petrovietnam": "cross-env NODE_ENV=test jest --testPathPattern=petrovietnam --coverage=false --verbose",
    "verify:petrovietnam": "cross-env NODE_ENV=test jest --testPathPattern=petrovietnam --coverage=false --verbose",
    "test:metasteel-api": "cross-env NODE_ENV=development node scripts/testMetaSteelEndpoints.js"
  },
  "dependencies": {
    "@google-cloud/documentai": "^8.8.0",
    "@google-cloud/pubsub": "^4.9.0",
    "@google-cloud/secret-manager": "^5.6.0",
    "@google-cloud/storage": "^7.14.0",
    "@google-cloud/tasks": "^6.2.1",
    "@google-cloud/vertexai": "^1.10.0",
    "@google/genai": "^1.34.0",
    "@google/generative-ai": "^0.24.1",
    "@sentry/node": "^10.28.0",
    "@sentry/profiling-node": "^10.28.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "html-pdf-node": "^1.0.8",
    "ioredis": "^5.8.2",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.9.8",
    "openai": "^6.9.1",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "cross-env": "^10.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/scripts/"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js",
      "**/?(*.)+(spec|test).js"
    ],
    "forceExit": false,
    "detectOpenHandles": false
  }
}
