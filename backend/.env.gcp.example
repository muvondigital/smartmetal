# ============================================
# GOOGLE CLOUD PLATFORM CONFIGURATION
# ============================================
# Complete GCP Migration - Azure services replaced with GCP equivalents
# Migration Date: 2025-12-30

# GCP Project Configuration
GCP_PROJECT_ID=your-gcp-project-id
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json

# Vertex AI (replaces Azure OpenAI)
VERTEX_AI_LOCATION=us-central1
VERTEX_AI_MODEL=gemini-1.5-pro-002

# Document AI (replaces Azure Document Intelligence)
DOCUMENT_AI_PROCESSOR_ID=your-processor-id
DOCUMENT_AI_LOCATION=us

# Cloud Storage (replaces Azure Blob Storage)
GCS_RFQ_BUCKET=pricer-rfq-documents
GCS_EXTRACTED_BUCKET=pricer-extracted-data

# Cloud Pub/Sub (replaces Azure Service Bus)
PUBSUB_EXTRACTION_TOPIC=document-extraction-topic
PUBSUB_EXTRACTION_SUBSCRIPTION=document-extraction-sub
PUBSUB_PARSING_TOPIC=ai-parsing-topic
PUBSUB_PARSING_SUBSCRIPTION=ai-parsing-sub

# Redis (Memorystore for Redis or self-hosted)
REDIS_HOST=10.x.x.x
REDIS_PORT=6379
REDIS_ENABLED=false

# ============================================
# DATABASE CONFIGURATION
# ============================================

DATABASE_URL=postgresql://user:password@host:port/database

# ============================================
# APPLICATION CONFIGURATION
# ============================================

PORT=3001
NODE_ENV=production
FRONTEND_URL=http://localhost:5173

# ============================================
# AUTHENTICATION
# ============================================

JWT_SECRET=your-jwt-secret-key
JWT_EXPIRES_IN=24h

# ============================================
# EMAIL/SMTP CONFIGURATION
# ============================================

SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-password
SMTP_FROM=your-email@gmail.com

# ============================================
# DOCUMENT PROCESSING
# ============================================

MAX_PDF_PAGES_TO_PROCESS=100
ENABLE_DI_CHUNKED_FALLBACK=true
DI_CHUNK_SIZE=10
DI_PARTIAL_RESULT_THRESHOLD=5

# ============================================
# FEATURE FLAGS
# ============================================

ENABLE_ADVANCED_DUTY_RULES=true
ENABLE_REGULATORY_INTELLIGENCE=true
ENABLE_COMPLIANCE_CENTER=false
ENABLE_LANDED_COST_V2=false
ENABLE_AI_REGULATORY_ASSISTANT=false
ENABLE_TIMING_LOGS=false
PRICE_AGREEMENT_STRICT_MODE=false
